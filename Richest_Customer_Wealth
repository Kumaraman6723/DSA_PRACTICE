public class Richest_Customer_Wealth {
    public static void main(String[] args) {

    }
    public static int maximumWealth(int[][] accounts) {
        int max=0;
        for(int[] a:accounts)
        {
            for(int n:a)
            {
                int sum=0;
                sum=sum+n;
                if(sum>max)
                {
                    max=sum;
                }
            }
            System.out.println();
        }
        return max;

    }
}
